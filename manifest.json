{
  "manifest_version": 2,

  "name": "Cookies pop-up killer",
  "description": "This extension kills the annoying cookie alert.",
  "version": "1.0",
  "icons": { "16": "icon16.png",
           "48": "icon48.png"
           },
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.1.4.min.js", "popup.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}